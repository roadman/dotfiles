## git
alias gitdf="git diff"
alias gitst="git status"
alias gitpsm="git push origin master"
alias gitps="git push origin"
alias gitpl="git pull --prune"
#alias gitlg="git log"
#alias gitlg="git log --graph --name-status --relative-date --decorate --color"
alias gitlg="git log --graph --name-status --decorate --color"
alias gitlgo="git log --oneline --decorate"
alias gitlgf="git log --name-status"
alias gitcm="git commit"
alias gitcmae="git commit --amend"
alias gitckd="git checkout develop"
alias gitckf="git checkout -"
alias gitfe="git fetch --all"
alias gitbr="git branch -a"
